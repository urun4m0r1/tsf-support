{
    "name": "tsf-support",
    "displayName": "Korean Novel Text Script Format",
    "description": "VSCode를 활용한 한국어 소설 작성 지원 확장.",
    "version": "1.0.0",
    "publisher": "rekorn",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "git://github.com/urun4m0r1/tsf-support.git"
    },
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Other"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tsf",
                "aliases": [
                    "TSF",
                    "tsf"
                ],
                "extensions": [
                    ".tsf",
                    ".tsf.txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsf",
                "scopeName": "source.tsf",
                "path": "./syntaxes/tsf.tmLanguage.json"
            }
        ]
    }
}